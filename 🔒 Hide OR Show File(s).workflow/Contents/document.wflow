				<key>ActionBundlePath</key>
				<string>/System/Library/Automator/Run AppleScript.action</string>
				<key>ActionName</key>
				<string>Run AppleScript</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>on run {input, parameters}		set button_1 to "Cancel"	set button_2 to "Show"	set button_3 to "Hide"		repeat with x from 1 to count of input		set theSource to item x of input				--Get base filename + extension		set text item delimiters to ":"		set theFilename to last text item of (theSource as string) --if file		if (theFilename is "") then set theFilename to text item ((count of text item of (theSource as string)) - 1) of (theSource as string) --if folder				--Get parent path of file		set parentPath to FindAndReplaceText of (POSIX path of theSource) from theFilename to ""				if x is 1 then			if item 1 of theFilename is "." then set defaultButton to button_2			if item 1 of theFilename is not "." then set defaultButton to button_3						tell application "System Events"				activate				set dialogBox to display dialog "What do you want to do with the file(s)?" with title "Finder" with icon 0 buttons {button_1, button_2, button_3} Â¬					default button defaultButton cancel button 1				set theAnswer to button returned of dialogBox			end tell		end if				--Hide File(s)		if theAnswer is "Hide" then			do shell script "chflags hidden " &amp; quoted form of POSIX path of theSource						if item 1 of theFilename is not "." then				set originalFile to POSIX path of theSource				set renamedFile to parentPath &amp; "." &amp; theFilename				renameFile(originalFile, renamedFile)			end if		end if				--Show File(s)		if theAnswer is "Show" then			do shell script "chflags nohidden " &amp; quoted form of POSIX path of theSource						if item 1 of theFilename is "." then				set theFilename to text 2 thru (count of theFilename) of theFilename				set originalFile to POSIX path of theSource				set renamedFile to parentPath &amp; theFilename				renameFile(originalFile, renamedFile)			end if		end if	end repeatend runon renameFile(originalFile, renamedFile)	do shell script "mv " &amp; (quoted form of POSIX path of originalFile) &amp; space &amp; (quoted form of POSIX path of renamedFile)end renameFileon FindAndReplaceText of t from s to r	set d to text item delimiters	set text item delimiters to s	set t to t's text items	set text item delimiters to r	tell t to set t to beginning &amp; ({""} &amp; rest)	set text item delimiters to d	tend FindAndReplaceText</string>
