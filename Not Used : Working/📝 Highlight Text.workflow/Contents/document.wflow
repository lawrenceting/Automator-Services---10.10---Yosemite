				<dict>
					<key>source</key>
					<string>on run {input, parameters}		tell application "Microsoft Word"		activate		set windowName to name of active document	end tell		try		tell application "System Events" to tell process "Microsoft Word"			tell window windowName				tell tab group 1					tell group 1						click (first menu button whose help is "Text Highlight Color")					end tell				end tell			end tell		end tell			on error error_message number error_number				if error_message is "System Events got an error: Canâ€™t get tab group 1 of window 1 of process \"Microsoft Word\". Invalid index." and error_number is -1719 then			activate			display dialog "No text selected." giving up after 1		end if	end tryend run</string>
